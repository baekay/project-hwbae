<!DOCTYPE sqlMapConfig
	PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<!-- properties resource="sfmi/component/squad/itquality/dao/sqlmap/maps/oracle-sql-map-config.properties"/ -->
	<properties resource="sfmi/common/dao/sqlmap/oracle-sql-map-config.properties"/>

	<settings
	cacheModelsEnabled="true"
	enhancementEnabled="true"
	lazyLoadingEnabled="true"
	errorTracingEnabled="false"
	maxRequests="${maxRequests}"
	maxSessions="${maxSessions}"
	maxTransactions="${maxTransactions}"
	useStatementNamespaces="false"
	/>

	<transactionManager type="${transaction-manager-type}" commitRequired="true">
		<property name="UserTransaction" value="${user-transaction}"/>
		<dataSource type="${data-source-type}">
			<property name="DataSource" value="${itso-data-source}"/>
			<property name="JDBC.Driver" value="${driver}"/>
			<property name="JDBC.ConnectionURL" value="${url}"/>
			<property name="JDBC.Username" value="${username}"/>
			<property name="JDBC.Password" value="${password}"/>
			<property name="Driver.currentSchema" value="${currentSchema}"/>
		</dataSource>
	</transactionManager>
